<input type="text" id="search" placeholder="Buscar usuario...">
<table>
  {{#each users}}
    <tr>
      <td>{{this._id}}</td>
      <td>{{this.first_name}}</td>
      <td>{{this.last_name}}</td>
      <td>{{this.email}}</td>
      <td>{{this.role}}</td>
      <td>{{this.last_connection}}</td>

      <td><a href="/admin/users/edit/{{this._id}}">Editar</a></td>
      <td><a href="/admin/users/{{this._id}}">Eliminar</a></td>
    </tr>
  {{/each}}
</table>     